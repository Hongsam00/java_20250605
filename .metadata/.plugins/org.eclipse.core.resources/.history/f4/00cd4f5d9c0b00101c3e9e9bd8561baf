package com.pcwk.ehr.ed02;

public class Ed04_ArraySort {

	public static void main(String[] args) {
		//[5, 3, 8, 4, 2]
		int[] arr = {5, 3, 8, 4, 2};
		
		//버블 Sort
		for(int i = 0; i < arr.length-1; i++) {//전체 반복(arr.length-1)
			
			for(int j = 0; j < arr.length-1; i++) {
				
			}
			
		}
		
		
		//결과 출력
		for(int num : arr) {
			System.out.print(num + ",");
		}
		
	}

}
