package com.pcwk.ehr.ed03;

import java.util.*;

public class Ed01 {

	public static void main(String[] args) {
		//성적 기준으로 내림차순 정렬
		// "김철수" : 87, "이영희" : 92, "박민수" : 75

		Map<String, Integer> scores = Map.of("김철수", 87, "이영희", 92, "박민수", 75);
		
		for ( String key : scores.keySet()) {
			System.out.println(key + ", " +scores.get(key));
		}
		
		//List 변환
		scores.entrySet()
		//sort()
		
	}

}
